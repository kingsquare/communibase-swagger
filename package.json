{
  "name": "communibase-swagger",
  "version": "0.1.0",
  "description": "Generate Swagger compatible definitions for Communibase API",
  "repository": "https://github.com/kingsquare/communibase-swagger.git",
  "author": "Kingsquare BV <source@kingsquare.nl>",
  "license": "ISC",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "tsc",
    "test": "nyc ava"
  },
  "bin": {
    "cbswagger": "node ./dist/cli.js"
  },
  "files": [
    "README.md",
    "dist/**/*"
  ],
  "dependencies": {
    "@types/swagger-schema-official": "^2.0.18",
    "bluebird": "^3.7.0",
    "communibase-connector-js": "^1.0.5",
    "swagger-schema-official": "^2.0.0-bab6bed"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/lodash": "^4.14.142",
    "ava": "^2.4.0",
    "nyc": "^14.1.1",
    "prettier": "^1.15.3",
    "ts-node": "^8.4.1",
    "tslint": "^5.12.0",
    "tslint-config-kingsquare": "^5.0.1",
    "typescript": "^3.2.2"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "test/**/*",
      "!test/**/fixtures"
    ],
    "sources": [
      "src/**/*"
    ]
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "include": [
      "src/**/*.ts"
    ]
  }
}
